{
  "name": "magenx404",
  "version": "1.0.1",
  "main": "./index.js",
  "type": "module",
  "description": "magen 404 implementation - modular authentication features",
  "author": "mageninfra",
  "license": "ISC",
  "keywords": [
    "solana",
    "authentication",
    "wallet",
    "token-gated",
    "x404"
  ],
  "files": [
    "src/",
    "index.js",
    "README.md",
    "src/types/index.d.ts"
  ],
  "dependencies": {
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@vitest/ui": "^1.6.1",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^1.6.1"
  },
  "scripts": {
    "start": "tsx server/index.ts",
    "dev": "tsx watch server/index.ts",
    "build": "tsc",
    "demo": "python3 -m http.server 8000 || python -m http.server 8000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:server": "vitest run tests/server",
    "test:client": "vitest run tests/client",
    "lint": "echo 'No linter configured. Add ESLint if needed.'"
  },
  "exports": {
    ".": {
      "types": "./src/types/index.d.ts",
      "import": "./index.js"
    },
    "./blacklist": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/blacklist.js"
    },
    "./timelock": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/timelock.js"
    },
    "./multitoken": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/multitoken.js"
    },
    "./activity": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/activity.js"
    },
    "./tier": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/tier.js"
    },
    "./nodebt": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/nodebt.js"
    },
    "./age": {
      "types": "./src/types/index.d.ts",
      "import": "./src/features/age.js"
    },
    "./utils": {
      "types": "./src/types/index.d.ts",
      "import": "./src/utils/index.js"
